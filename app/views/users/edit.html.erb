<%= form_with(model: @user, url: user_path(params[:id]), local: true) do |f| %>
  <p>
  <%= image_tag @user.image.url %>
  <%= f.file_field :image %>
  </p>
  <p>
  <%= f.label :name %>
  <%= f.text_field :name %>
  </p>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.submit "編集する" %>
<% end %>